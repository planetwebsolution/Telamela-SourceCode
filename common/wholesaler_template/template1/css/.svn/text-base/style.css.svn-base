body {
	font-family: 'Open Sans',arial,sans-serif,Helvetica,monospace;
	background: #f1f1f1;
}
@font-face {
    font-family: 'akzidenz-grotesk_bq_extrabold';
    src: url('akzidenzgrotesk-extrabold-webfont.eot');
    src: url('akzidenzgrotesk-extrabold-webfont.eot?#iefix') format('embedded-opentype'),
         url('akzidenzgrotesk-extrabold-webfont.woff') format('woff'),
         url('akzidenzgrotesk-extrabold-webfont.ttf') format('truetype'),
         url('akzidenzgrotesk-extrabold-webfont.svg#akzidenz-grotesk_bq_extrabold') format('svg');
    font-weight: normal;
    font-style: normal;

}

*{
	margin:0;
	padding:0;
}
.clear{
	float:none;
	width:100%;
	display: block;
	height:1px;
	clear: both;
}
a img{
	border:none;
}

.outer_container{
	width:784px;
	margin:0 auto;
	background: #fff;
}
.header{
	width:100%;
	float:left;
	height:93px;
	background: url("../images/header.jpg") repeat scroll 0 0 transparent;
	text-align: center;
	
	}
a.logo img{
	float:none;
	max-height:93px !important;
}
.menu{
	display:none;
}	
.menu h3{
	width:100%;
	float:left;
	text-indent:10%;
	line-height: 45px;
	cursor: pointer;
}

.nav{
	background-color:#5b576d;
	float: left;
	width:100%;
}
.nav ul{
	 list-style:none;
	 }
.nav ul li{
	float:left;
	}
	.nav ul li.activeli{
	background-color:#fff;
	}
	.nav ul li.activeli a{
	color:#000;
	}
.nav ul li a{
	color:#fff;
	line-height:46px;
	text-decoration: none;
	font-size: 15px;
	font-weight: 300;
	padding:0 28px;
	display: block;
	}
.nav ul li a:hover{
	background: #fff;
	color:#000;
}	
.slide_section{
	width:100%;
	float:left;
}	
.products_outer{
	width:100%;
	float:left;
	background: #ffffff;
	padding-top: 40px;
}
.products_box{
	width:95.61%;
	float:left;
	padding:0 2% 0 2.5%;
}
.my_product{
	width:235px;
	float:left;
	margin-left:20px;
	margin-bottom:20px;
}
.mlz{
	margin-left:0;
}
.pro_img{
	width:100%;
	float: left;
	background: #f1f1f1;
}
.img_center{
	text-align: center;
	vertical-align: middle;
	display:table-cell;
	width:235px;
	height:210px;
	
}
.img_center img{
	display:block;
	margin:0 auto;
	max-width:100% !important;
}
.pro_details{
	background: #504f54;
	width:100%;
	float:left;
	min-height:71px;
	
}
.pro_details .fleft{
	width:98%;
	padding-left:2%;
	float: left;
	
}
.pro_details .fright{
	width:98%;
	padding-left:2%;
	float: left;
	
	padding-top:4px;
	color: #f89600;
	font-size:13px;
	font-weight:300;
	line-height:20px; 
}
.pro_details .fright small{
text-decoration:line-through;
font-size:17px;
	font-weight:300;
	
}
.pro_details .fright strong{
	font-size:23px;
	font-weight:500;
	

}
.pro_details .fleft h3{
	font-size:18px;
	font-weight:700;
	line-height:21px;
	color:#fff;
	margin:8px 0;
	}
.pro_details .fleft h3 a{
color:inherit;
text-decoration:none;
}
.pro_details .fleft h3 a:hover{
	text-decoration:underline;
	}
.pro_details .fleft span{
	font-size:12px;
	font-weight:400;
	line-height:16px;
	color:#fff;
	
}	
.pro_details .fleft p{
	line-height:16px;
}
.content{
	width:96%;
	float:left;
	padding:35px 2% 23px;

	
}
.min-height-box{
	min-height:868px;
	float:left;
	width:100%;
	margin-bottom:20px;
}
.ndis{display:none}
.content h2.heading{
	color: #1d6da2;
	font-size:24px;
	line-height:28px;
	font-weight: 500; 
	width:100%;
	float:left;
	margin-bottom:22px;
}
.content p{
	color:#454545;
	font-size:16px;
	float:left;
	width:100%;
	font-weight:300;
	line-height: 30px;
	margin-bottom:15px;
}
 .content > ul{
 	padding-left:30px !important;
	float:left;
	
 } 
 .content li{
	color:#454545;
	font-size:16px;
	float:left;
	width:100%;
	font-weight:300;
	line-height: 30px;
	margin-bottom:5px;
 	 
 
 }
 .content p label{
 font-weight:600;
 margin-right:10px;
 }

.footer{
	float:left;
	width:96.1%;
	background: #e7e7e7;
	min-height:56px;
	padding:20px 2%;

}
.footerlinks{
	margin-bottom:20px;
	float:left;
	width:100%;
}
.footerlinks li{
	list-style: none;
	float:left;
}
.footerlinks li a{
	font-size:13px;
	color:#454545;
	font-weight: 400;
	text-decoration: none;
	padding:0 8px;
}
.footerlinks li a:hover{
	color:#000;
}
p.copyright{
	color: #999999;
	font-size:13px;
	padding-left:8px;
}
.item{
	position:relative;
	cursor: move;
}
h2.big{
	position:absolute;
	top:107px;
	left:49%;
	color:#fff;
	 font-family: 'akzidenz-grotesk_bq_extrabold';
	 font-size: 30px;
	 line-height:36px;
	 max-width:49%;
}
